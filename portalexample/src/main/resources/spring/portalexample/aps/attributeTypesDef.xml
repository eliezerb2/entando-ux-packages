<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xmlns:context="http://www.springframework.org/schema/context"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	
	<bean id="EnumeratorMapAttribute" class="org.entando.entando.aps.system.common.entity.model.attribute.EnumeratorMapAttribute" 
			parent="MonoTextAttribute">
		<property name="type" value="EnumeratorMap" />
		<property name="handler" >
			<bean class="com.agiletec.aps.system.common.entity.parse.attribute.MonoTextAttributeHandler" />
		</property>
	</bean>
        
	<bean id="enumeratorMapAttributeContentWrapper" class="com.agiletec.aps.system.common.entity.loader.ExtraAttributeWrapper" >
		<property name="attribute" ref="EnumeratorMapAttribute" />
		<property name="entityManagerNameDest"><value>jacmsContentManager</value></property>
	</bean>
	
</beans>